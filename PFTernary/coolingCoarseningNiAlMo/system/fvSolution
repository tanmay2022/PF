FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


solvers
{
    
    mu_1
    {
        solver                    GAMG;
        preconditioner            FDIC;
        smoother                symGaussSeidel;//DICGaussSeidel;
        mergeLevels                1;
        agglomerator            faceAreaPair;
        nCellsInCoarsestLevel    50;
        tolerance                1e-8;
        relTol                    0.1;
    }
    
    mu_2
    {
        solver                    GAMG;
        preconditioner            FDIC;
        smoother                symGaussSeidel;//DICGaussSeidel;
        mergeLevels                1;
        agglomerator            faceAreaPair;
        nCellsInCoarsestLevel    50;
        tolerance                1e-8;
        relTol                    0.1;
    }

	phi
    {
        solver                    GAMG;
        preconditioner            FDIC;
        smoother                symGaussSeidel;//DICGaussSeidel;
        mergeLevels                1;
        agglomerator            faceAreaPair;
        nCellsInCoarsestLevel    50;
        tolerance                1e-8;
        relTol                    0.1;
    }

    theta
    {
        solver                    GAMG;
        preconditioner            FDIC;
        smoother                symGaussSeidel;//DICGaussSeidel;
        mergeLevels                1;
        agglomerator            faceAreaPair;
        nCellsInCoarsestLevel    50;
        tolerance                1e-8;
        relTol                    0.1;
    }
    
    D
    {
        solver                    GAMG;
        preconditioner            FDIC;
        smoother                DICGaussSeidel;
        mergeLevels                1;
        agglomerator            faceAreaPair;
        nCellsInCoarsestLevel    100;
        tolerance               1e-5;
        relTol                    0.9;
    }
}
