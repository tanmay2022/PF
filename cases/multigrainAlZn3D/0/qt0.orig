//============== CYCLIC BOUNDARY CONDITIONS ====================//

boundaryField
{
    posXBound
    {
        type            cyclic;
    }
    negXBound
    {
        type            cyclic;
    }
    posYBound
    {
        type            cyclic;
    }
    negYBound
    {
        type            cyclic;
    }
    posZBound
    {
        type            cyclic;
    }
    negZBound
    {
        type            cyclic;
    }
}



